    "start": "node index.js",
    "wm-update": "node_modules/protractor/bin/webdriver-manager update",
    "sass": "sass --watch public/graph/css/style.scss:public/graph/css/style.css"
  },
  "author": "Planetary Science Institute",
  "repository": {
    "type": "git",
    "url": "https://github.com/sbn-psi/ldd-transform"
  },
  "license": "ISC",
