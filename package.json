{
  "name": "ldd-transform",
  "version": "2.4.1",
  "description": "A tool for transforming PDS4 Local Data Dictionary Ingest Files (Ingest_LDD) between simple types. Includes an interactive graph for creating and modifying Ingest_LDD files.",
  "main": "index.js",
  "scripts": {
    "docker-ssh": "docker exec -it ldd-transform /bin/bash",
    "docker-tail": "docker exec -it harvest-server cat /proc/19/fd/2",
